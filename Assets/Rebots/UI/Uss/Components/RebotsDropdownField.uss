.unity-base-popup-field__input {
    background-color: rgb(255, 255, 255);
    border-left-color: rgb(188, 188, 188);
    border-right-color: rgb(188, 188, 188);
    border-top-color: rgb(188, 188, 188);
    border-bottom-color: rgb(188, 188, 188);
    color: rgb(0, 0, 0);
    padding-left: 15px;
    padding-right: 15px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-bottom: 0;
    padding-top: 0;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

.unity-disabled .unity-base-popup-field__input {
    opacity: 1;
    background-color: rgb(244, 244, 245);
    color: rgb(188, 188, 188);
}

.rebots-validation .unity-base-popup-field__input {
    border-left-color: rgb(215, 5, 18);
    border-right-color: rgb(215, 5, 18);
    border-top-color: rgb(215, 5, 18);
    border-bottom-color: rgb(215, 5, 18);
}

